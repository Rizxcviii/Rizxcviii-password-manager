{
  "name": "Rizxcviiis' Password Manager",
  "description": "A password management extension that generates USEFUL passwords",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": ["storage"],
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Enter your vault (Remember your keyword...?)"
  },
  "background": {
    "scripts": ["background.js"],
    "persist": false
  },
  "icons": {
    "16": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  }
}
